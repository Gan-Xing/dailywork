# Copy to .env.local and fill in the Supabase credentials from the project settings.

# Connection pooling endpoint (use Supabase's POSTGRES_PRISMA_URL value).
DATABASE_URL="postgres://postgres.<project-ref>:<password>@aws-1-us-east-1.pooler.supabase.com:6543/postgres?sslmode=require&pgbouncer=true"

# Direct endpoint for Prisma migrations (Supabase POSTGRES_URL_NON_POOLING).
DIRECT_DATABASE_URL="postgres://postgres.<project-ref>:<password>@aws-1-us-east-1.pooler.supabase.com:5432/postgres?sslmode=require"

DEEPSEEK_API_KEY="your-deepseek-api-key"
